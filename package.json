{
  "name": "sysora-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "nodemon server/index.js",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "node server/index.js",
    "start:prod": "NODE_ENV=production node server/index.js",
    "test": "echo \"No tests specified\" && exit 0",
    "deploy": "chmod +x deploy.sh && ./deploy.sh",
    "docker:build": "docker build -t sysora-platform .",
    "docker:run": "docker run -p 3000:3000 --env-file .env sysora-platform",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "health": "curl -f http://localhost:3000/api/health || exit 1"
  },
  "dependencies": {
    "@aws-sdk/client-ecr": "^3.821.0",
    "@aws-sdk/client-ecs": "^3.821.0",
    "@aws-sdk/client-s3": "^3.821.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "framer-motion": "^12.15.0",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.0",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.263.1",
    "mime-types": "^3.0.1",
    "mongoose": "^7.0.3",
    "multer": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-hotkeys-hook": "^5.1.0",
    "react-router-dom": "^6.8.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.6.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.5"
  }
}
